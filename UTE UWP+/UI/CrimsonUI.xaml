                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="-4"
                            Background="{ThemeResource ComboBoxBackgroundFocused}"
                            BorderBrush="{ThemeResource ComboBoxBackgroundBorderBrushFocused}"
                            BorderThickness="{StaticResource ComboBoxBackgroundBorderThicknessFocused}"
                            CornerRadius="{StaticResource ComboBoxHiglightBorderCornerRadius}"
                            Opacity="0" />
                        <ToolkitControls:DropShadowPanel Grid.Column="0" HorizontalAlignment="Left"
                            Grid.ColumnSpan="2" BlurRadius="16.0" Canvas.ZIndex="2" MinHeight="2" ShadowOpacity="0.43" IsMasked="True" Grid.Row="1"
                            OffsetX="0.0" OffsetY="0.0" VerticalAlignment="Bottom" RenderTransformOrigin="0.5, 0.5" Color="{ThemeResource SystemAccentColorLight3}" MinWidth="0.1">
                            <ToolkitControls:DropShadowPanel.RenderTransform>
                                <ScaleTransform x:Name="BottomBar" ScaleX="0" ScaleY="1"/>
                            </ToolkitControls:DropShadowPanel.RenderTransform>
                            <Grid Grid.ColumnSpan="2" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" RenderTransformOrigin="0.5, 0.5" Width="{TemplateBinding Width}" Height="2" CornerRadius="0, 0, 4, 4">
                                <Grid.Background>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                                        <GradientStop Color="{ThemeResource SystemAccentColorLight2}" Offset="1" />
                                        <GradientStop Color="{ThemeResource SystemAccentColorLight3}" Offset="0" />
                                    </LinearGradientBrush>
                                </Grid.Background>
                            </Grid>
                        </ToolkitControls:DropShadowPanel>
                        <Border x:Name="Background"
